{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "bezhansalleh/filament-panel-switch": "dev-main",
        "corvmc/bands": "*",
        "corvmc/equipment": "*",
        "corvmc/events": "*",
        "corvmc/finance": "*",
        "corvmc/kiosk": "*",
        "corvmc/membership": "*",
        "corvmc/moderation": "*",
        "corvmc/space-management": "*",
        "corvmc/sponsorship": "*",
        "corvmc/support": "*",
        "filament/filament": "^5.0",
        "filament/notifications": "^5.0",
        "filament/spatie-laravel-media-library-plugin": "^5.0",
        "filament/spatie-laravel-tags-plugin": "^5.0",
        "finller/laravel-money": "^2.2",
        "google/apiclient": "^2.18",
        "internachi/modular": "^2.3",
        "knplabs/github-api": "^3.16",
        "l3aro/filament-turnstile": "^1.0",
        "laravel/cashier": "^15.7",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.3",
        "laravel/tinker": "^2.10.1",
        "league/flysystem-aws-s3-v3": "^3.29",
        "lorisleiva/laravel-actions": "^2.9",
        "maatwebsite/excel": "^3.1",
        "mews/purifier": "^3.4",
        "monicahq/laravel-cloudflare": "^4.0",
        "php-http/guzzle7-adapter": "^1.1",
        "propaganistas/laravel-phone": "^6.0",
        "rlanvin/php-rrule": "^2.6",
        "secondnetwork/blade-tabler-icons": "^3.34",
        "sentry/sentry-laravel": "^4.16",
        "spatie/laravel-activitylog": "^4.10",
        "spatie/laravel-data": "^4.17",
        "spatie/laravel-medialibrary": "^11.13",
        "spatie/laravel-model-flags": "^1.4",
        "spatie/laravel-model-states": "^2.12",
        "spatie/laravel-permission": "^6.21",
        "spatie/laravel-query-builder": "^6.3",
        "spatie/laravel-settings": "^3.4",
        "spatie/laravel-sluggable": "^3.7",
        "spatie/laravel-tags": "^4.10",
        "spatie/period": "^2.4",
        "spatie/sheets": "^1.12",
        "staudenmeir/laravel-adjacency-list": "^1.25",
        "stechstudio/filament-impersonate": "^4.0",
        "symfony/http-client": "^7.3",
        "symfony/postmark-mailer": "^7.3",
        "zenstruck/commonmark-extensions": "1.x-dev"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.6",
        "fakerphp/faker": "^1.23",
        "filament/upgrade": "^5.0",
        "larastan/larastan": "^3.8",
        "laravel/boost": "^1.1",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2",
        "phpmd/phpmd": "^2.15",
        "phpstan/phpstan": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan icons:sync-tabler --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test --parallel"
        ],
        "test:coverage": [
            "@php artisan config:clear --ansi",
            "XDEBUG_MODE=coverage php artisan test --coverage --parallel"
        ],
        "test:smoke": [
            "@php artisan config:clear --ansi",
            "@php artisan test --group=smoke --parallel"
        ],
        "phpmd": [
            "phpmd app ansi phpmd.xml"
        ],
        "phpmd:html": [
            "phpmd app html phpmd.xml --report-file phpmd-report.html"
        ],
        "phpstan": [
            "phpstan analyse --memory-limit=512M"
        ],
        "phpstan:baseline": [
            "phpstan analyse --memory-limit=512M --generate-baseline"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/DevonCash/filament-panel-switch"
        },
        {
            "type": "path",
            "url": "app-modules/*",
            "options": {
                "symlink": true
            }
        }
    ]
}
